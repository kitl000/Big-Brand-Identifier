{
  "name": "AoG BBi",
  "description": "Brand checker",
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": ["storage"],
  "background": {
    "scripts": ["options.js", "js/jquery.min.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
